name: '{{ profile }}'
type: profile
version: 8.x-dev
{% if description %}
description: '{{ description }}'
{% endif %}
core: '8.x'
{% if install %}
install:
{% for module in install %}
  - {{ module }}
{% endfor %}
{% endif %}

themes:
  - bartik
  - seven

base profile: lightning
{% if exclude %}
exclude:
{% for module in exclude %}
  - {{ module }}
{% endfor %}
{% endif %}
